{
  "manifest_version": 2,

  "name": "Travala price ticker",
  "version": "0.1.3",
  "description": "Persistent price ticker for travala",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Travala price ticker",  
    "default_popup": "popup.html"
  },

  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },

  "background": {
  "scripts":["ticker.js"]
  }
}
